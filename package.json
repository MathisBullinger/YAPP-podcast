{
  "name": "yapp-podcast",
  "version": "1.0.0",
  "description": "YAPP Podcast Service",
  "scripts": {
    "start:offline": "sls offline",
    "logs": "sls logs -f podcast",
    "logs:watch": "sls logs -f podcast -t",
    "deploy": "sls deploy",
    "deploy:function": "sls deploy function -f podcast",
    "prettier:check": "prettier --check \"./*.{js,ts}\" \"./**/*.{js,ts}\"",
    "prettier:write": "prettier --write \"./*.{js,ts}\" \"./**/*.{js,ts}\"",
    "eslint:check": "eslint . --ext .js,.ts,",
    "eslint:write": "eslint . --ext .js,.ts, --fix",
    "lint": "npm run eslint:check && npm run prettier:check",
    "format": "npm run eslint:write && npm run prettier:write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathisBullinger/YAPP-podcast.git"
  },
  "author": "Mathis Bullinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MathisBullinger/YAPP-podcast/issues"
  },
  "homepage": "https://github.com/MathisBullinger/YAPP-podcast#readme",
  "dependencies": {
    "@apollo/federation": "^0.7.0",
    "@babel/runtime": "^7.6.0",
    "@sentry/node": "^5.5.0",
    "apollo-server-lambda": "^2.6.7",
    "axios": "^0.19.0",
    "dynamite": "^0.10.1",
    "graphql": "^14.4.0",
    "sanitize-html": "^1.20.1",
    "sax": "^1.2.4",
    "striptags": "^3.1.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cache-loader": "^4.1.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "^3.1.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "serverless-offline": "^5.7.2",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.3",
    "webpack": "^4.35.0",
    "webpack-node-externals": "^1.7.2"
  }
}
