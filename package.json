    "format:write": "prettier --write \"./*.js\" \"./**/*.js\"",
    "lint:check": "eslint . --ext .js",
    "lint:write": "eslint . --ext .js --fix",
    "format-lint:check": "npm run lint:check && npm run format:check",
    "format-lint:write": "npm run lint:write && npm run format:write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathisBullinger/YAPP-podcast.git"
