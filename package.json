    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.2",
    "serverless-offline": "^5.7.2",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.35.0",
    "ts-loader": "^6.0.4",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
