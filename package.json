  },
  "homepage": "https://github.com/MathisBullinger/YAPP-podcast#readme",
  "dependencies": {
    "@apollo/federation": "^0.7.0",
    "@sentry/node": "^5.5.0",
    "apollo-server-lambda": "^2.6.7",
    "axios": "^0.19.0",
    "dynamite": "^0.10.1",
    "graphql": "^14.4.0",
    "sax": "^1.2.4",
    "uuid": "^3.3.2"
